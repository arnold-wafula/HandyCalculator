<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="16dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/theme_rose"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:background="@drawable/circular_button" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/theme_scarlet"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/theme_rose"
        android:background="@drawable/circular_button" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/theme_orange"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/theme_scarlet"
        android:background="@drawable/circular_button" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/cantaloupe"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:background="@drawable/circular_button"
        android:layout_marginStart="16dp"
        app:layout_constraintStart_toEndOf="@id/theme_orange"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/macaroon"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/theme_rose"
        app:layout_constraintStart_toStartOf="parent"
        android:background="@drawable/circular_button" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/lemon_yellow"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/theme_rose"
        app:layout_constraintStart_toEndOf="@id/macaroon"
        android:background="@drawable/circular_button" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/green"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/theme_rose"
        app:layout_constraintStart_toEndOf="@id/lemon_yellow"
        android:background="@drawable/circular_button" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/crocodile"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/theme_rose"
        app:layout_constraintStart_toEndOf="@id/green"
        android:background="@drawable/circular_button" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/og_blue"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/macaroon"
        app:layout_constraintStart_toStartOf="parent"
        android:background="@drawable/circular_button" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/sky_blue"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/macaroon"
        app:layout_constraintStart_toEndOf="@id/og_blue"
        android:background="@drawable/circular_button" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/indigo"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/macaroon"
        app:layout_constraintStart_toEndOf="@id/sky_blue"
        android:background="@drawable/circular_button" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/cerulean"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/macaroon"
        app:layout_constraintStart_toEndOf="@id/indigo"
        android:background="@drawable/circular_button" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/purple"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/og_blue"
        app:layout_constraintStart_toStartOf="parent"
        android:background="@drawable/circular_button" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/mauve"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/og_blue"
        app:layout_constraintStart_toEndOf="@id/purple"
        android:background="@drawable/circular_button" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/fuscia"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/og_blue"
        app:layout_constraintStart_toEndOf="@id/mauve"
        android:background="@drawable/circular_button" />

    <androidx.appcompat.widget.AppCompatImageButton
        android:id="@+id/strawberry"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/og_blue"
        app:layout_constraintStart_toEndOf="@id/fuscia"
        android:background="@drawable/circular_button" />

</androidx.constraintlayout.widget.ConstraintLayout>